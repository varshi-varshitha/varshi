{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-mvarshitha-dev"
		},
		"lnk_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lnk_adls'"
		},
		"lnk_azuresql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lnk_azuresql'"
		},
		"lnk_selfhost_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lnk_selfhost'"
		},
		"lnk_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://varshithadatalakedev.dfs.core.windows.net/"
		},
		"lnk_azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "db-server-varshitha.database.windows.net"
		},
		"lnk_azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_varshitha"
		},
		"lnk_azuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin_varshitha"
		},
		"lnk_selfhost_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "VARSHITHA"
		},
		"lnk_selfhost_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2022"
		},
		"lnk_selfhost_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DRS1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "df-mvarshitha-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnk_adls_accountKey": {
						"value": "[parameters('lnk_adls_accountKey')]"
					},
					"lnk_azuresql_password": {
						"value": "[parameters('lnk_azuresql_password')]"
					},
					"lnk_adls_properties_typeProperties_url": {
						"value": "[parameters('lnk_adls_properties_typeProperties_url')]"
					},
					"lnk_azuresql_properties_typeProperties_server": {
						"value": "[parameters('lnk_azuresql_properties_typeProperties_server')]"
					},
					"lnk_azuresql_properties_typeProperties_database": {
						"value": "[parameters('lnk_azuresql_properties_typeProperties_database')]"
					},
					"lnk_azuresql_properties_typeProperties_userName": {
						"value": "[parameters('lnk_azuresql_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "df-mvarshitha-dev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnk_selfhost_password": {
						"value": "[parameters('lnk_selfhost_password')]"
					},
					"lnk_selfhost_properties_typeProperties_server": {
						"value": "[parameters('lnk_selfhost_properties_typeProperties_server')]"
					},
					"lnk_selfhost_properties_typeProperties_database": {
						"value": "[parameters('lnk_selfhost_properties_typeProperties_database')]"
					},
					"lnk_selfhost_properties_typeProperties_userName": {
						"value": "[parameters('lnk_selfhost_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'df-mvarshitha-dev_ArmTemplate_0')]"
			]
		}
	]
}